'''
def min(x,y):
    if x<=y:
        return x
    else:
        return y 
k =min(3,6)
print(k)
t= min(min(3,6),8)
print(t)'''

'''
def f(*n):
    m=n[0]
    for x in n:
        if m >x:
            m=x
            return m
print(f(2,4,6,9,8))
'''


