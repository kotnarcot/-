n=input("ДОВОД")
j=len(n)-1
i=0
is_palindrom = True
while i<j:
 if n[i]!= n[j]:
  is_palindrom = False
 i+= 1
 j -= 1
if is_palindrom:
  print("YES")
else:
  print("NO")
